<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.marondalgram.like.dao.LikeDAO">
	<select id="checkPostLikedByUserIdAndPostId" parameterType="com.marondalgram.like.model.Like" resultType="com.marondalgram.like.model.Like">
		SELECT
			`likePost`
		FROM
			`like`
		WHERE
			`userId` = #{userId}
		AND
			`postId` = #{postId}
	</select>
</mapper>
